@model Presentation.Web.Colpatria.Models.ErrorViewModel
@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="text-center">@Model.Title</h2>
<p class="text-center">@Model.Message</p>
<form method="POST" action="@Url.Action("LogOff", "Account")">
    @Html.AntiForgeryToken()
    <p class="text-center">
        <input type="submit" value="Volver a la página inicial" class="btn btn-blue" />
    </p>
</form>