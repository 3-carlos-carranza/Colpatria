@model dynamic
<div class="table-responsive">
    <table class="table table-hover">
        <tbody>
            <tr>
                <td>Nombre:</td>
                <th>@Model.UserInfoDto.FullName</th>
            </tr>
            <tr>
                <td>Documento:</td>
                <th>@Model.UserInfoDto.IdentificationType : @Model.UserInfoDto.Identification</th>
            </tr>
            <tr>
                <td>Expedición:</td>
                <th>@Model.UserInfoDto.DateOfExpedition.ToShortDateString()</th>
            </tr>
            <tr>
                <td>Lugar:</td>
                <th>@Model.UserInfoDto.ExpeditionPlace</th>
            </tr>
            <tr>
                <td>Dirección</td>
                <th>@Model.UserInfoDto.Address</th>
            </tr>
            <tr>
                <td>Correo</td>
                <th>@Model.UserInfoDto.Email</th>
            </tr>
            <tr>
                <td>Teléfono</td>
                <th>@Model.UserInfoDto.Telephone</th>
            </tr>
            <tr>
                <td>Celular</td>
                <th>@Model.UserInfoDto.Cellphone</th>
            </tr>
        </tbody>
    </table>
</div>